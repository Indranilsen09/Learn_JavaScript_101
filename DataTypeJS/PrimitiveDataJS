//Integer
var a = 10;
//Float
var b = 44.34;
//negative-int
var c= -9;

console.log("Integer: "+ a);
console.log("Float: "+ b);
console.log("negative-int: "+c);

//Strings
var s1 = "Hello World";
console.log("Strings in Double-inverted commas: "+ s1);

//String inside Single-inverted commas
var s2= 'Hello World';
console.log("'Strings': "+ s2);

//String with other vars
var age= 24;
var s3 = `Indra is of ${age} age`;
console.log(s3);