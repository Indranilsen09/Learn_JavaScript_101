/* Unary Operator have 2 Operation
    1. Increment
    2. Decrement
Increment : Increase the Value By 1
//PostFix: First Assign Then Increment
//PreFix: First Increment the Assign
*/
console.log('Increment Unary Operator');
var a =2, b=2;
console.log(a++); //2
console.log(++b); //3

//Decrement : Decrease the Value By 1
//PostFix: First Assign Then Decrement
//PreFix: First Decrement then Assign
console.log('Decrement Unary Operator');
var c= 3, d=5;
console.log(c--); //3
console.log(--d); //4