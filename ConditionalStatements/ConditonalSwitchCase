var val =9;

switch(val)
{
    case val:1;
            console.log('Its Monday');
            break;
    case val:2;
            console.log('Its Tuesday');
            break;
    case val:3;
            console.log('Its WednesDay');
            break;
    case val:4;
            console.log('Its Thursday');
            break;
    case val:5;
            console.log('Its Friday');
            break;
    case val:6;
            console.log('Its Saturday');
            break;
    case val:7;
            console.log('Its Sunday');
            break;
        default: console.log("Which country are you in?");
}